{
  "name": "@zikkado/vanta.js",
  "version": "0.0.1",
  "description": "3D animated backgrounds for your website (TypeScript)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "src/types/vanta.d.ts",
  "files": [
    "dist",
    "src/types"
  ],
  "scripts": {
    "build": "node ./webpack.build.js && lessc src/styles.less gallery/styles.css",
    "prepublishOnly": "npm run build",
    "watch": "npm run build && chokidar \"src/*.*\" \"webpack.config.js\" -c \"npm run build\"",
    "dev": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1mZ1kk4d0/zikka-vanta.git"
  },
  "keywords": [
    "animation",
    "3d",
    "three",
    "threejs",
    "background",
    "canvas",
    "typescript",
    "webgl",
    "vanta"
  ],
  "author": "Teng Bao",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./src/types/vanta.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "devDependencies": {
    "glslify": "^6.1.0",
    "glslify-loader": "^2.0.0",
    "less": "^3.9.0",
    "raw-loader": "^0.5.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
